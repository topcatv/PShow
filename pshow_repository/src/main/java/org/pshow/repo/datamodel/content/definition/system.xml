<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://pshow.org/content/definition/1.0" name="sys:systemmodel">
    <description>Alfresco Repository System Definitions</description>
    <author>Alfresco</author>
    <published>2005-09-29</published>
    <version>1.0</version>
    <imports>
        <namespace uri="http://www.pshow.org/model/datatype/0.1" prefix="d"/>
    </imports>
    <namespaces>
        <namespace uri="www.pshow.org/model/system/0.1" prefix="sys"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="sys:deleted">
            <title>Deleted</title>
            <description>Placeholder type for deleted nodes</description>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </type>
        <type name="sys:base">
            <title>Base</title>
            <properties/>
            <associations/>
            <mandatory-facets>
                <facet>sys:referenceable</facet>
                <facet>sys:localized</facet>
            </mandatory-facets>
        </type>
        <type name="sys:descriptor">
            <title>Descriptor</title>
            <parent>sys:base</parent>
            <properties>
                <property name="sys:name">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionMajor">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionMinor">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionRevision">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionLabel">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:licenseMode">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionBuild">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionSchema">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                    <default>0</default>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionEdition">
                    <type>d:content</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionProperties">
                    <type>d:content</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <mandatory-facets/>
        </type>
        <type name="sys:container">
            <title>Container</title>
            <parent>sys:base</parent>
            <properties/>
            <associations>
                <association name="sys:children">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>sys:base</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <mandatory-facets/>
        </type>
        <type name="sys:store_root">
            <title>Store Root</title>
            <parent>sys:container</parent>
            <properties/>
            <associations>
                <association name="sys:lost_found">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sys:lost_found</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
            <mandatory-facets>
                <facet>sys:facet_root</facet>
            </mandatory-facets>
        </type>
        <!-- <type name="sys:reference">
            <title>Reference</title>
            <parent>sys:base</parent>
            <properties>
                <property name="sys:reference">
                    <type>d:noderef</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <mandatory-facets/>
        </type> -->
        <type name="sys:lost_found">
            <title>Lost+Found</title>
            <parent>sys:container</parent>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </type>
    </types>
    <facets>
        <facet name="sys:facet_root">
            <title>Root</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:referenceable">
            <title>Referenceable</title>
            <properties>
                <property name="sys:store-protocol">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:store-identifier">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:node-uuid">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:node-dbid">
                    <type>d:long</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:incomplete">
            <title>Incomplete</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:temporary">
            <title>Temporary</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:hidden">
            <title>Hidden</title>
            <properties>
                <property name="sys:clientVisibilityMask">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <default>0</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:undeletable">
            <title>Undeletable</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:noContent">
            <title>NoContent</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
        <facet name="sys:webdavNoContent">
            <title>NoContent</title>
            <properties/>
            <associations/>
            <mandatory-facets/>
        </facet>
